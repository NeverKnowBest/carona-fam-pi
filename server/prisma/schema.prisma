generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model users {
  id         Int      @id @default(autoincrement())
  name       String
  cpf        String   @unique
  ra         Int      @unique
  password   String
  created_at DateTime @default(now())
}